<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<testPlan>
  <planSettings/>
  <planFeatures>
    <planFeature enabled="true" name="PDF" type="OUTPUT">
      <settings mutable="Mutable">
        <namedValue name="enabled">
          <value class="value" valueClass="boolean">true</value>
        </namedValue>
        <namedValue name="includeTestCaseSummary">
          <value class="value" valueClass="boolean">true</value>
        </namedValue>
        <namedValue name="includeAllFailuresInSummary">
          <value class="value" valueClass="boolean">true</value>
        </namedValue>
        <namedValue name="includeTestCaseDetails">
          <value class="value" valueClass="boolean">true</value>
        </namedValue>
        <namedValue name="includeOnlyFailures">
          <value class="value" valueClass="boolean">false</value>
        </namedValue>
        <namedValue name="includeOnlyBdd">
          <value class="value" valueClass="boolean">false</value>
        </namedValue>
        <namedValue name="includeSkippedTestSteps">
          <value class="value" valueClass="boolean">false</value>
        </namedValue>
        <namedValue name="includeTestCasePathHierarchy">
          <value class="value" valueClass="boolean">true</value>
        </namedValue>
        <namedValue name="includeTestCaseDescription">
          <value class="value" valueClass="boolean">true</value>
        </namedValue>
        <namedValue name="includeTestStepTime">
          <value class="value" valueClass="boolean">true</value>
        </namedValue>
        <namedValue name="includeTestCaseWarningMessages">
          <value class="value" valueClass="boolean">true</value>
        </namedValue>
        <namedValue name="includeTestCaseInfoMessages">
          <value class="value" valueClass="boolean">false</value>
        </namedValue>
        <namedValue name="includeTestCaseDebugMessages">
          <value class="value" valueClass="boolean">false</value>
        </namedValue>
        <namedValue name="includeTestCaseScreenshots">
          <value class="value" valueClass="boolean">true</value>
        </namedValue>
        <namedValue name="includeFullScreenShot">
          <value class="value" valueClass="boolean">false</value>
        </namedValue>
        <namedValue name="artifactName">
          <value class="value" valueClass="string">executionReport.pdf</value>
        </namedValue>
      </settings>
    </planFeature>
    <planFeature enabled="true" name="EMAIL" type="NOTIFICATION">
      <settings mutable="Mutable">
        <namedValue name="to">
          <value class="value" valueClass="string"/>
        </namedValue>
        <namedValue name="cc">
          <value class="value" valueClass="string"/>
        </namedValue>
        <namedValue name="subject">
          <value class="value" valueClass="string">Test Plan Execution</value>
        </namedValue>
        <namedValue name="emailBody">
          <value class="value" valueClass="string">Test Plan Run Report</value>
        </namedValue>
      </settings>
    </planFeature>
    <planFeature enabled="true" name="PIECHART" type="OUTPUT">
      <settings mutable="Mutable">
        <namedValue name="enabled">
          <value class="value" valueClass="boolean">true</value>
        </namedValue>
        <namedValue name="includeLegend">
          <value class="value" valueClass="boolean">true</value>
        </namedValue>
        <namedValue name="artifactName">
          <value class="value" valueClass="string">pieChart.png</value>
        </namedValue>
      </settings>
    </planFeature>
  </planFeatures>
</testPlan>
